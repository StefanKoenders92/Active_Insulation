EnergyManagementSystem:Program,
    SetMode,                 !- Name
    IF OpTempZone < HeatingTemp,  !- Program Line 1
    RUN HeatingControlSouth, !- Program Line 2
    RUN HeatingControlEast,  !- A4
    RUN HeatingControlNorth, !- A5
    RETURN,                  !- A6
    ELSEIF OpTempZone > CoolingTemp,  !- A7
    RUN CoolingControlSouth, !- A8
    RUN CoolingControlEast,  !- A9
    RUN CoolingControlNorth, !- A10
    RETURN,                  !- A11
    ENDIF;                   !- A12

EnergyManagementSystem:Program,
    SetTemp,                 !- Name
    SET ControlTempCoolingHigh = 24,  !- Program Line 1
    SET ControlTempCoolingLow = 22,  !- Program Line 2
    SET ControlTempHeatingHigh = 22,  !- A4
    SET ControlTempHeatingLow = 20,  !- A5
    SET HeatingTemp = ControlTempHeatingHigh,  !- A6
    SET CoolingTemp = ControlTempCoolingLow;  !- A7

EnergyManagementSystem:Subroutine,
    HeatingControlSouth,     !- Name
    IF OpTempZone < (HeatingTemp - 0.1),  !- Program Line 1
    SET HeatingTemp = ControlTempHeatingHigh,  !- Program Line 2
    IF SurfTemp_South > OpTempZone,  !- A4
    SET Z_S_FAN_MFR = 1.0,   !- A5
    ELSEIF SurfTemp_South < OpTempZone,  !- A6
    SET Z_S_FAN_MFR = 0.0,   !- A7
    ENDIF,                   !- A8
    RETURN,                  !- A9
    ELSEIF OpTempZone > (HeatingTemp - 0.1),  !- A10
    SET HeatingTemp = ControlTempHeatingLow,  !- A11
    SET Z_S_FAN_MFR = 0.0,   !- A12
    ENDIF,                   !- A13
    RETURN;                  !- A14

EnergyManagementSystem:Subroutine,
    CoolingControlSouth,     !- Name
    IF OpTempZone > (CoolingTemp + 0.1),  !- Program Line 1
    SET CoolingTemp = ControlTempCoolingLow,  !- Program Line 2
    IF SurfTemp_South < OpTempZone,  !- A4
    SET Z_S_FAN_MFR = 1.0,   !- A5
    ELSEIF SurfTemp_South > OpTempZone,  !- A6
    SET Z_S_FAN_MFR = 0.0,   !- A7
    ENDIF,                   !- A8
    RETURN,                  !- A9
    ELSEIF OpTempZone < (CoolingTemp + 0.1),  !- A10
    SET CoolingTemp = ControlTempCoolingHigh,  !- A11
    SET Z_S_FAN_MFR = 0.0,   !- A12
    ENDIF,                   !- A13
    RETURN;                  !- A14

